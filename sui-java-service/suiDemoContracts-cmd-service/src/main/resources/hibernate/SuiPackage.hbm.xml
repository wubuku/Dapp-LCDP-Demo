<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.suidemocontracts.sui.contract">

  <class name="SuiPackage" table="sui_package">
	<id name="name" length="200" column="name"><generator class="assigned"/></id>
    <property name="objectId" ><column name="object_id" length="42"/></property>
    <property name="version" />
    <property name="publisher" ><column name="publisher" length="42"/></property>
	<property name="createdBy" ><column name="created_by" /></property>
	<property name="updatedBy" ><column name="updated_by" /></property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>

</hibernate-mapping>

