<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.suidemocontracts.sui.contract">

  <class name="SuiIdGeneratorDataObject" table="sui_id_generator_data_object">
	<id name="objectName" length="200" column="object_name"><generator class="assigned"/></id>
    <property name="suiObjectId" ><column name="sui_object_id" length="42"/></property>
    <property name="suiObjectType" ><column name="sui_object_type" length="200"/></property>
	<property name="createdBy" ><column name="created_by" /></property>
	<property name="updatedBy" ><column name="updated_by" /></property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>

</hibernate-mapping>

