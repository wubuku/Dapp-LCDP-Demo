<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.suidemocontracts.domain.daysummary">
  
  <class name="org.dddml.suidemocontracts.domain.daysummary.AbstractDaySummaryState$SimpleDaySummaryState" table="day_summary">

	<composite-id name="day" class="org.dddml.suidemocontracts.domain.Day">
		<key-property name="monthYearNumber" ><column name="day_month_year_number" /></key-property>
		<key-property name="monthYearCalendar" ><column name="day_month_year_calendar" length="20" /></key-property>
		<key-property name="monthNumber" ><column name="day_month_number" /></key-property>
		<key-property name="monthIsLeap" ><column name="day_month_is_leap" /></key-property>
		<key-property name="number" ><column name="day_number" /></key-property>
		<key-property name="timeZone" ><column name="day_time_zone" length="20" /></key-property>
	</composite-id>
	<property name="version" column="version" type="long" />

	<property name="id_" ><column name="id" unique-key="unique_daysummary_sid" sql-type="VARCHAR(42)" /></property>
	<property name="description" ><column name="description" /></property>
	<property name="metadata" ><column name="metadata" /></property>
	<property name="optionalData" ><column name="optional_data" /></property>
	<property name="createdBy" ><column name="created_by" /></property>
	<property name="updatedBy" ><column name="updated_by" /></property>
	<property name="active" ><column name="active" /></property>
	<property name="deleted" ><column name="deleted" /></property>

    <property name="createdAt" column="created_at" />

    <property name="updatedAt" column="updated_at" />

      <set name="arrayData" table="day_summary_array_data" lazy="false">
<key>

<column name="day_month_year_number" />
<column name="day_month_year_calendar" />
<column name="day_month_number" />
<column name="day_month_is_leap" />
<column name="day_number" />
<column name="day_time_zone" />
</key>				
<element column="array_data_item" type="string" not-null="true" />

      </set>

            

  </class>
  
</hibernate-mapping>

