<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.suidemocontracts.domain.domainname">
  
  <class name="org.dddml.suidemocontracts.domain.domainname.AbstractDomainNameState$SimpleDomainNameState" table="domain_name">

	<composite-id name="domainNameId" class="org.dddml.suidemocontracts.domain.domainname.DomainNameId">
		<key-property name="topLevelDomain" ><column name="domain_name_id_top_level_domain" /></key-property>
		<key-property name="secondLevelDomain" ><column name="domain_name_id_second_level_domain" /></key-property>
	</composite-id>
	<property name="version" column="version" type="long" />

	<property name="expirationDate" ><column name="expiration_date" /></property>
	<property name="createdBy" ><column name="created_by" /></property>
	<property name="updatedBy" ><column name="updated_by" /></property>
	<property name="active" ><column name="active" /></property>
	<property name="deleted" ><column name="deleted" /></property>

    <property name="createdAt" column="created_at" />

    <property name="updatedAt" column="updated_at" />


  </class>
  
</hibernate-mapping>

