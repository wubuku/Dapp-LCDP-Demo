<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.suidemocontracts.domain.player">
  <class name="org.dddml.suidemocontracts.domain.player.AbstractPlayerState$SimplePlayerState" table="player">
    <id name="playerId" length="66">
      <column name="player_id" sql-type="VARCHAR(66)" />
      <generator class="assigned" />
    </id>
    <version name="offChainVersion" column="off_chain_version" type="long" />
    <property name="id_">
      <column name="id" unique-key="unique_player_sid" sql-type="VARCHAR(66)" />
    </property>
    <property name="nickname">
      <column name="nickname" length="50" />
    </property>
    <property name="intro">
      <column name="intro" length="350" />
    </property>
    <property name="version">
      <column name="version" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="active">
      <column name="active" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
